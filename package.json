{
  "name": "atom-transpiler-debug-tool",
  "version": "1.0.4",
  "description": "A debug utility for packages using Atom's \"custom transpiler\" feature",
  "author": "smhxx <captaintrek@gmail.com>",
  "bin": "dist/index.js",
  "keywords": [
    "atom",
    "transpile",
    "transpiler",
    "debug",
    "tool",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smhxx/atom-transpiler-debug-tool.git"
  },
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "deploy": "semantic-release",
    "lint": "tslint -p src/tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^2.2.0",
    "glob": "^7.1.2",
    "resolve": "^1.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^4.0.0",
    "@types/glob": "^5.0.0",
    "@types/node": "^9.0.0",
    "@types/resolve": "^0.0.6",
    "semantic-release": "^15.0.0",
    "tslint": "^5.0.0",
    "tslint-config-airbnb": "^5.0.0",
    "typescript": "^2.0.0"
  },
  "license": "MIT",
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "breaking": true,
          "release": "major"
        },
        {
          "type": "feat",
          "release": "minor"
        },
        {
          "type": "fix",
          "release": "patch"
        },
        {
          "type": "perf",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        }
      ]
    },
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
